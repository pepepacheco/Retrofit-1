<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vcarmen.es.academia"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme" >
        <activity android:name="vcarmen.es.academia.view.activities.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="vcarmen.es.academia.view.activities.ActivityCreateAlumno" />
        <activity android:name="vcarmen.es.academia.view.activities.ActivityUpdateAlumno" />
        <activity android:name="vcarmen.es.academia.view.activities.ActivityCreateAsignatura" />
        <activity android:name="vcarmen.es.academia.view.activities.ActivityUpdateAsignatura" />
    </application>

</manifest>